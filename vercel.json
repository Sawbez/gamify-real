{
    "version": 2,
    "builds": [
        {
            "buildCommand": "cd front && npm run build && cd .. ",
            "outputDirectory": "front/dist",
            "installCommand": "cd front && npm i",
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}